{
  "name": "eternal-bot",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.cjs",
    "build": "tsup src/index.ts --minify",
    "deploy-commands": "tsx src/scripts/deploy-commands.ts",
    "delete-commands": "tsx src/scripts/delete-commands.ts",
    "migrate": "node-pg-migrate -d -m ./migrations --no-reject-unauthorized",
    "migrate-create": "node-pg-migrate create -m ./migrations"
  },
  "dependencies": {
    "@crowdin/crowdin-api-client": "^1.45.0",
    "@pinecone-database/pinecone": "^6.1.1",
    "bottleneck": "^2.19.5",
    "cld3-asm": "^4.0.0",
    "csvtojson": "^2.0.10",
    "decompress": "^4.2.1",
    "deepl-node": "^1.18.0",
    "diff": "^8.0.2",
    "discord-giveaways": "^6.0.1",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "franc-min": "^6.2.0",
    "fuse.js": "^7.1.0",
    "fuzzysort": "^3.1.0",
    "googleapis": "^152.0.0",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.28.2",
    "ms": "^2.1.3",
    "node-fetch": "^2.7.0",
    "openai": "^5.6.0",
    "p-limit": "^6.2.0",
    "p-map": "^4.0.0",
    "pg": "^8.16.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/decompress": "^4.2.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/memoizee": "^0.4.12",
    "@types/ms": "^2.1.0",
    "@types/node-fetch": "^2.6.12",
    "@types/pg": "^8.15.2",
    "node-pg-migrate": "^8.0.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
